{
  "name": "reactexpress-test",
  "version": "1.0.0",
  "description": "testing react teasting",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "debug": "NODE_ENV=debug jest",
    "debug-specific": "echo NYI; exit -1; NODE_ENV=debug jest --grep"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  }
}
